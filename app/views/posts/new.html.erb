<h2>New Post</h2>

<div class="row">
<div class="col-md-6 col-md-offset-3">
<%= form_for [@user, @post], html: { multipart: true } do |f| %>



<div class="form-group">
  <%= f.label :content %>
  <%= f.text_area :content, class: 'form-control'  %>
</div>

<div class="form-group">
<%= f.submit "Create" , class: "btn btn-primary" %>
 </div>
<% end %>
